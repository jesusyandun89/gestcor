@model IEnumerable<GestCor.Models.Ytbl_ProgCorteModels>

@{
    ViewBag.Title = "Listado de cortes programados";
}
<h2>Listado de cortes programados</h2>
<p>
    @Html.ActionLink("Crear nuevo", "UploadFile")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Document_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Customer_Number_Upload)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nick_User)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date_Programed)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date_Upload)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsValid)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Document_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Customer_Number_Upload)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nick_User)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date_Programed)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date_Upload)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsValid)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id })
        </td>
    </tr>
}

</table>
