@model IEnumerable<GestCor.Models.ContextReport>

@{
    ViewBag.Title = "Result";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Resultados</h2>

@using (Html.BeginForm("ExportToExcel", "ProgramaCorte", FormMethod.Post))
{
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.cpartyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cpartyAccountId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.idProgCorte)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdBloque)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContBloque)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ejeDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.cpartyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cpartyAccountId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.idProgCorte)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdBloque)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContBloque)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ejeDate)
        </td>
        <td>
            @Html.ActionLink("Exportar a Excel", "ExportToExcel", new { id = item.idProgCorte }) |
        </td>
    </tr>
}
    <tr>
        <td colspan="4">
            <br />
            <br />
            <input type="submit" value="Export to Excel" class="button" />
        </td>
    </tr>  
</table>

}
